{
	"name": "jstewmc/php-helpers",
	"description": "Static classes to help with numbers, strings, arrays, files, and bools in PHP",
	"license": "MIT",
	"keywords": ["PHP", "helper", "number", "string", "array", "file", "boolean"],
	"authors": [
		{
      "name": "Contributors",
      "homepage": "https://github.com/jstewmc/php-helpers/contributors"
    }
	],
	"require": {
		"php": "^7.3 || ^8.0"
	},
	"require-dev": {
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^9",
		"roave/security-advisories": "dev-latest",
    "slevomat/coding-standard": "^6"
	},
	"autoload": {
		"psr-4": {
			"Jstewmc\\PhpHelpers\\": "src/"
		}
	}
}
